{
  "manifest_version": 3,
  "name": "Personalized News Feed",
  "version": "1.0",
  "description": "A customizable news feed for your new tab page",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}